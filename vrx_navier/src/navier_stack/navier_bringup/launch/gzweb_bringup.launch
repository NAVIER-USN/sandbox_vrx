<?xml version="1.0"?>
<launch>
  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find vrx_gazebo)/config/custom_rosconsole.conf"/>
  <arg name= "urdf" default="/opt/overlay_ws/src/sandbox/vrx_navier/src/navier_wamv/navier_wamv.urdf"/>
  <arg name="namespace" default="wamv"/>
  <arg name="wamv_locked" default="false" />
  <arg name="paused" default="false"/>
  <arg name="pinger_params" default="$(find vrx_gazebo)/config/pinger.yaml"/>

  <arg name="x" default="-532" />
  <arg name="y" default="162" />
  <arg name="z" default="0.1" />
  <arg name="P" default="0" />
  <arg name="R" default="0" />
  <arg name="Y" default="1" />



  <!-- Start Gazebo with the world file -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name"   value="$(find vrx_gazebo)/worlds/gymkhana.world" />
    <arg name="verbose"      value="true"/>
    <arg name="paused"       value="$(arg paused)"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="headless"     value="true" />
    <arg name="gui"          value="false" />
  </include>


  <!-- Spawn model in Gazebo -->
  <node name="spawn_model" pkg="gazebo_ros" type="spawn_model"
        args="-x $(arg x) -y $(arg y) -z $(arg z)
              -R $(arg R) -P $(arg P) -Y $(arg Y)
              -file /opt/overlay_ws/src/sandbox/vrx_navier/src/navier_wamv/navier_wamv.sdf
              -sdf 
              -model wamv"/>
  <param name = "robot_description" command="$(find xacro)/xacro --inorder '$(arg urdf)'"/>
  <node ns="wamv" pkg="robot_state_publisher" type="robot_state_publisher" name="rob_st_pub"/>

</launch>
